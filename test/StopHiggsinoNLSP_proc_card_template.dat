import model_v4 MODEL

# Define multiparticle labels
define p = u u~ c c~ d d~ s s~ b b~ g
define j = p
define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

# Specify process(es) to run
generate p p > t1 t1~ @1
add process p p > t1 t1~ j @2
add process p p > t1 t1~ j j @3

# Output processes to MadEvent directory
output MODEL

# To generate events, you can go to the created directory and 
# run ./bin/generate_events
